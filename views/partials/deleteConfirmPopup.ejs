<div id="deleteConfirm" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Bạn chắc chắn muốn xoá?</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <p>Xoá người dùng <strong id="username"></strong></p>
          <div id="loading" class="loader" hidden></div>
          <div id="modal-error-message" class="alert alert-danger" hidden></div>
          <div id="dataId" hidden></div>
        </div>
        <div class="modal-footer">
          <button id="btnYes" type="button" class="btn btn-primary">Đồng ý</button>
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
        </div>
      </div>
    </div>
  </div>